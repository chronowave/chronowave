JAR=antlr-4.8-complete.jar
BASEDIR=$(dirname "$0")

java -jar $BASEDIR/$JAR SSQL.g4
javac -cp $BASEDIR/$JAR SSQL*.java
java -cp $BASEDIR/$JAR:. org.antlr.v4.gui.TestRig SSQL start -gui
